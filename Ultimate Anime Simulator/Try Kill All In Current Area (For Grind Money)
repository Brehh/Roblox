local main = game:GetService("Players").LocalPlayer.ServerPlayerData.PlayerAreaData.Value
local HttpService = game:GetService("HttpService")
local data = HttpService:JSONDecode(main)
local area = data.CurrentAreaName

-- This script was generated by Hydroxide's RemoteSpy: https://github.com/Upbolt/Hydroxide
for i,v in next,workspace.Monsters[area]:GetChildren() do
    game.Players.LocalPlayer.Character.PrimaryPart.CFrame = v.PrimaryPart.CFrame
    wait(0.15)
    local ohString1 = "OneKillMonster"
    local ohInstance2 = v

    game:GetService("ReplicatedStorage").RemoteEvents.MonsterRemote:FireServer(ohString1, ohInstance2)
    wait(0.1)
end
