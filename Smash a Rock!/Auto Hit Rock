-- This code was generated by Cobalt
-- https://github.com/notpoiu/cobalt

function getnearest()
	local bruh = 25
	local m_distance = bruh
	local obj
	for a,b in next,workspace.GameFolder.Worlds:GetChildren() do
		if not b:FindFirstChild("Spawner") then
			continue
		end
		for i,v in next,b.Spawner:GetChildren() do
			if not v:FindFirstChild("Object") then
				continue
			end
			if v.Object:GetAttribute("Health") == 0 then
				continue
			end
			local distance = (v.Object.PrimaryPart.Position - game.Players.LocalPlayer.Character.PrimaryPart.Position).Magnitude
			if distance < bruh-1 and distance < m_distance then
				m_distance = distance
				obj = v.Object
			end
		end
	end
	return obj
end

while wait() do
	local got,have = pcall(getnearest)

	if got and have and game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool") then
		while have:GetAttribute("Health") ~= 0 and game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool")  do  
			if game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool") then
				local Event = game:GetService("ReplicatedStorage").Events.Attack
				Event:FireServer(
					have,
					game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA("Tool"),
					{
						Timer = 0.2
					}
				)
			end
			wait()
		end
	end
end
